<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<converter>
		<description></description>
		<display-name>Date Converter</display-name>
		<converter-id>dateConverter</converter-id>
		<converter-class>com.rfp.converter.DateConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>loginMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.LoginMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.RegisterUserMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rfpSearchMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.RFPSearchMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rfpViewMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.ViewRFPMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rfpSectionsMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.AdminRFPSectionsMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sectionRegisterMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.SectionRegisterMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>RFPStatusRegisterMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.RFPStatusRegisterMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listRFPSectionRoles</managed-bean-name>
		<managed-bean-class>com.rfp.bean.ListRFPSectionRolesMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>RegisterUserView</display-name>
		<from-view-id>/RegisterUserView.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/SuccessRegisterUser.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>faild</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/UserHome.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>successAdmin</from-outcome>
			<to-view-id>/adminHome.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
