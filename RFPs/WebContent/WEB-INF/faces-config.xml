<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<converter>
		<description></description>
		<display-name>Date Converter</display-name>
		<converter-id>dateConverter</converter-id>
		<converter-class>com.rfp.converter.DateConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>loginMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.LoginMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerRFPMB</managed-bean-name>
		<managed-bean-class>com.rfp.bean.RegisterRFPMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

</faces-config>
